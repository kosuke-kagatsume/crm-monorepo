{
  "name": "svc-customer-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["svc-customer"],
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "svc-customer-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["svc-customer:build", "svc-customer:serve"]
    }
  }
}
